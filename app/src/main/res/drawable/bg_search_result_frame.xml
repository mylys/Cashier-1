<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">


    <!-- solid 设置stroke设置的边框以内的颜色 -->
    <solid android:color="@color/color_text_white"/>
    <!-- corners 设置边框四角弧度 -->
    <corners android:radius="@dimen/x10"/>
    <!-- padding主要设置组件里内容距离组件内边框的间距 -->
    <padding android:left="@dimen/x5" android:top="@dimen/x5"
        android:right="@dimen/x5" android:bottom="@dimen/x5"/>


</shape>