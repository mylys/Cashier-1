<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/root"
    android:layout_width="@dimen/x386"
    android:layout_height="@dimen/y394"
    android:paddingStart="@dimen/x13"
    android:paddingEnd="@dimen/x13"
    android:paddingTop="@dimen/y21"
    android:paddingBottom="@dimen/x13"
    android:background="@drawable/ic_main_set_count">

    <View
        android:id="@+id/line1"
        android:layout_width="@dimen/x361"
        android:layout_height="@dimen/y2"
        android:background="@color/color_d3d3d3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/line2"/>

    <View
        android:id="@+id/line2"
        android:layout_width="@dimen/x361"
        android:layout_height="@dimen/y2"
        android:background="@color/color_d3d3d3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/line1"
        app:layout_constraintBottom_toTopOf="@id/line3"
        app:layout_constraintEnd_toEndOf="parent"/>

    <View
        android:id="@+id/line3"
        android:layout_width="@dimen/x361"
        android:layout_height="@dimen/y2"
        android:background="@color/color_d3d3d3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/line2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <!--竖线-->
    <View
        android:id="@+id/line4"
        android:layout_width="@dimen/x2"
        android:layout_height="@dimen/y360"
        android:background="@color/color_d3d3d3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/line5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <View
        android:id="@+id/line5"
        android:layout_width="@dimen/x2"
        android:layout_height="@dimen/y360"
        android:background="@color/color_d3d3d3"
        app:layout_constraintStart_toEndOf="@id/line4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>


     <!--数字-->
    <!--row-->
    <TextView
        android:id="@+id/tv_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        style="@style/SetCount"
        android:text="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="@id/line4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/line1"/>

    <TextView
        android:id="@+id/tv_2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        style="@style/SetCount"
        android:text="2"
        app:layout_constraintStart_toStartOf="@id/line4"
        app:layout_constraintEnd_toEndOf="@id/line5"
        app:layout_constraintTop_toTopOf="@id/tv_1"
        app:layout_constraintBottom_toBottomOf="@id/tv_1"/>

    <TextView
        android:id="@+id/tv_3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        style="@style/SetCount"
        android:text="3"
        app:layout_constraintStart_toStartOf="@id/line5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/tv_1"
        app:layout_constraintBottom_toBottomOf="@id/tv_1"/>

    <!--row-->
    <TextView
        android:id="@+id/tv_4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        style="@style/SetCount"
        android:text="4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="@id/line4"
        app:layout_constraintTop_toBottomOf="@id/line1"
        app:layout_constraintBottom_toTopOf="@id/line2"/>

    <TextView
        android:id="@+id/tv_5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        style="@style/SetCount"
        android:text="5"
        app:layout_constraintStart_toStartOf="@id/tv_2"
        app:layout_constraintEnd_toEndOf="@id/tv_2"
        app:layout_constraintTop_toTopOf="@id/tv_4"
        app:layout_constraintBottom_toBottomOf="@id/tv_4"/>

    <TextView
        android:id="@+id/tv_6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        style="@style/SetCount"
        android:text="6"
        app:layout_constraintStart_toStartOf="@id/tv_3"
        app:layout_constraintEnd_toEndOf="@id/tv_3"
        app:layout_constraintTop_toTopOf="@id/tv_4"
        app:layout_constraintBottom_toBottomOf="@id/tv_4"/>

    <!--row-->
    <TextView
        android:id="@+id/tv_7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        style="@style/SetCount"
        android:text="7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="@id/line4"
        app:layout_constraintTop_toBottomOf="@id/line2"
        app:layout_constraintBottom_toTopOf="@id/line3"/>

    <TextView
        android:id="@+id/tv_8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        style="@style/SetCount"
        android:text="8"
        app:layout_constraintStart_toStartOf="@id/tv_2"
        app:layout_constraintEnd_toEndOf="@id/tv_2"
        app:layout_constraintTop_toTopOf="@id/tv_7"
        app:layout_constraintBottom_toBottomOf="@id/tv_7"/>

    <TextView
        android:id="@+id/tv_9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        style="@style/SetCount"
        android:text="9"
        app:layout_constraintStart_toStartOf="@id/tv_3"
        app:layout_constraintEnd_toEndOf="@id/tv_3"
        app:layout_constraintTop_toTopOf="@id/tv_7"
        app:layout_constraintBottom_toBottomOf="@id/tv_7"/>

    <!--row-->
    <TextView
        android:id="@+id/tv_00"
        android:layout_width="0dp"
        android:layout_height="0dp"
        style="@style/SetCount"
        android:text="00"
        app:layout_constraintStart_toStartOf="@id/tv_1"
        app:layout_constraintEnd_toEndOf="@id/tv_1"
        app:layout_constraintTop_toBottomOf="@id/line3"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <TextView
        android:id="@+id/tv_0"
        android:layout_width="0dp"
        android:layout_height="0dp"
        style="@style/SetCount"
        android:text="0"
        app:layout_constraintStart_toStartOf="@id/tv_2"
        app:layout_constraintEnd_toEndOf="@id/tv_2"
        app:layout_constraintTop_toTopOf="@id/tv_00"
        app:layout_constraintBottom_toBottomOf="@id/tv_00"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_delete"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/tv_3"
        app:layout_constraintEnd_toEndOf="@id/tv_3"
        app:layout_constraintTop_toTopOf="@id/tv_00"
        app:layout_constraintBottom_toBottomOf="@id/tv_00">

        <ImageView
            android:layout_width="@dimen/x45"
            android:layout_height="@dimen/y45"
            android:src="@drawable/icon_cancel"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
