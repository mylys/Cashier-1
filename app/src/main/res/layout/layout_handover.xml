        android:id="@+id/right_line_1"
        android:layout_width="@dimen/x472"
        android:layout_height="@dimen/y1"
        android:background="@color/color_d3d3d3"
        app:layout_constraintTop_toTopOf="@id/line1"
        app:layout_constraintBottom_toBottomOf="@id/line1"
        app:layout_constraintStart_toStartOf="@id/tv_text_total_cash"
        app:layout_constraintEnd_toEndOf="@id/tv_text_total_cash" />


    <View
        android:layout_width="0dp"
        android:layout_height="@dimen/y1"
        android:background="@color/color_d3d3d3"
        app:layout_constraintTop_toTopOf="@id/line2"
        app:layout_constraintBottom_toBottomOf="@id/line2"
        app:layout_constraintStart_toStartOf="@id/right_line_1"
        app:layout_constraintEnd_toEndOf="@id/right_line_1" />

    <View
        android:id="@+id/right_line_3"
        android:layout_width="0dp"
        android:layout_height="@dimen/y1"
        android:background="@color/color_d3d3d3"
        app:layout_constraintTop_toTopOf="@id/line3"
        app:layout_constraintBottom_toBottomOf="@id/line3"
        app:layout_constraintStart_toStartOf="@id/right_line_1"
        app:layout_constraintEnd_toEndOf="@id/right_line_1" />

    <View
        android:id="@+id/right_line_4"
        android:layout_width="0dp"
        android:layout_height="@dimen/y1"
        android:background="@color/color_d3d3d3"
        android:layout_marginTop="@dimen/y70"
        app:layout_constraintStart_toStartOf="@id/right_line_1"
        app:layout_constraintEnd_toEndOf="@id/right_line_1"
        app:layout_constraintTop_toBottomOf="@id/right_line_3" />

    <View
        android:id="@+id/right_line_5"
        android:layout_width="0dp"
        android:layout_height="@dimen/y1"
        android:background="@color/color_d3d3d3"
        android:layout_marginTop="@dimen/y70"
        app:layout_constraintStart_toStartOf="@id/right_line_1"
        app:layout_constraintEnd_toEndOf="@id/right_line_1"
        app:layout_constraintTop_toBottomOf="@id/right_line_4" />




