        app:layout_constraintEnd_toEndOf="@id/tv_cash"
        app:layout_constraintTop_toTopOf="@id/tv_text_alipay"
        app:layout_constraintBottom_toBottomOf="@id/tv_text_alipay"/>

    <!--微信-->
    <TextView
        android:id="@+id/tv_text_wechat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_wechat"
        style="@style/DetailTextStyle"
        android:layout_marginBottom="@dimen/y22"
        app:layout_constraintStart_toStartOf="@id/tv_text_cash"
        app:layout_constraintBottom_toTopOf="@id/line3"/>

    <TextView
        android:id="@+id/tv_wechat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.00"
        style="@style/DetailTextStyle"
        app:layout_constraintEnd_toEndOf="@id/tv_cash"
        app:layout_constraintTop_toTopOf="@id/tv_text_wechat"
        app:layout_constraintBottom_toBottomOf="@id/tv_text_wechat"/>

    <!--会员钱包-->
    <TextView
        android:id="@+id/tv_text_member"
        style="@style/DetailTextStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_member_package"
        android:layout_marginBottom="@dimen/y22"
        app:layout_constraintStart_toStartOf="@id/tv_text_cash"
        app:layout_constraintBottom_toTopOf="@id/line4" />

    <TextView
        android:id="@+id/tv_member"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.00"
        style="@style/DetailTextStyle"
        app:layout_constraintEnd_toEndOf="@id/tv_cash"
        app:layout_constraintTop_toTopOf="@id/tv_text_member"
        app:layout_constraintBottom_toBottomOf="@id/tv_text_member"/>



    <!--银联-->
    <TextView
        android:id="@+id/tv_text_bank_card"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_bank_card"
        style="@style/DetailTextStyle"
        android:layout_marginBottom="@dimen/y22"
        app:layout_constraintStart_toStartOf="@id/tv_text_cash"
        app:layout_constraintBottom_toTopOf="@id/line5"/>

    <TextView
