        android:layout_marginBottom="@dimen/y22"
        app:layout_constraintStart_toStartOf="@id/tv_text_cash"
        app:layout_constraintBottom_toTopOf="@id/line7"/>

    <TextView
        android:id="@+id/tv_all_refund"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.00"
        style="@style/DetailTextStyle"
        app:layout_constraintEnd_toEndOf="@id/tv_cash"
        app:layout_constraintTop_toTopOf="@id/tv_text_all_refund"
        app:layout_constraintBottom_toBottomOf="@id/tv_text_all_refund"/>

    <!--左分割线-->
    <View
        android:id="@+id/left"
        android:layout_width="@dimen/x1"
        android:layout_height="@dimen/y617"
        android:background="@color/color_d3d3d3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/right"/>

    <!--右分割线-->
    <View
        android:id="@+id/right"
        android:layout_width="@dimen/x1"
        android:layout_height="0dp"
        android:background="@color/color_d3d3d3"
        app:layout_constraintTop_toTopOf="@id/left"
        app:layout_constraintBottom_toBottomOf="@id/left"
        app:layout_constraintStart_toEndOf="@id/left"
        app:layout_constraintEnd_toEndOf="parent" />

    <!--总单据数-->
    <TextView
        android:id="@+id/tv_text_total_order_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_total_order_count"
        style="@style/HandoverTextStyle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="@id/left"
        app:layout_constraintTop_toTopOf="@id/tv_text_total_sales"
        app:layout_constraintBottom_toBottomOf="@id/tv_text_total_sales"/>

    <TextView
        android:id="@+id/tv_total_order_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        style="@style/HandoverMoneyTextStyle"
        app:layout_constraintStart_toStartOf="@id/tv_text_total_order_count"
        app:layout_constraintEnd_toEndOf="@id/tv_text_total_order_count"
        app:layout_constraintTop_toTopOf="@id/tv_total_sales"
        app:layout_constraintBottom_toBottomOf="@id/tv_total_sales"/>

    <View
        android:id="@+id/left_line_1"
        android:layout_width="@dimen/x472"
        android:layout_height="@dimen/y1"
        android:background="@color/color_d3d3d3"
        app:layout_constraintTop_toTopOf="@id/line1"
        app:layout_constraintBottom_toBottomOf="@id/line1"
        app:layout_constraintStart_toStartOf="@id/tv_total_order_count"
        app:layout_constraintEnd_toEndOf="@id/tv_total_order_count" />

    <View
