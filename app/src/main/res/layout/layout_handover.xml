        android:layout_height="@dimen/y1"
        android:background="@color/color_d3d3d3"
        android:layout_marginTop="@dimen/y70"
        app:layout_constraintTop_toBottomOf="@id/left_line_1"
        app:layout_constraintStart_toStartOf="@id/left_line_1"
        app:layout_constraintEnd_toEndOf="@id/left_line_1" />

