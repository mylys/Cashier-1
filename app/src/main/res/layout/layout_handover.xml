        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_reserver"
        style="@style/DetailTextStyle"
        android:layout_marginStart="@dimen/x13"
        app:layout_constraintStart_toStartOf="@id/right_line_1"
        app:layout_constraintTop_toTopOf="@id/tv_text_cash"
        app:layout_constraintBottom_toBottomOf="@id/tv_text_cash"/>

    <TextView
        android:id="@+id/tv_reserver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.00"
        style="@style/DetailTextStyle"
        android:layout_marginEnd="@dimen/x13"
        app:layout_constraintEnd_toEndOf="@id/right_line_1"
        app:layout_constraintTop_toTopOf="@id/tv_text_cash"
        app:layout_constraintBottom_toBottomOf="@id/tv_text_cash"/>

    <!--实收金额-->
