        android:id="@+id/tv_text_sale_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_sale_count"
        style="@style/DetailTextStyle"
        android:layout_marginStart="@dimen/x13"
        app:layout_constraintStart_toStartOf="@id/left_line_1"
        app:layout_constraintTop_toTopOf="@id/tv_text_cash"
        app:layout_constraintBottom_toBottomOf="@id/tv_text_cash"/>

    <TextView
        android:id="@+id/tv_sale_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        style="@style/DetailTextStyle"
        android:layout_marginEnd="@dimen/x13"
        app:layout_constraintEnd_toEndOf="@id/left_line_1"
        app:layout_constraintTop_toTopOf="@id/tv_text_sale_count"
        app:layout_constraintBottom_toBottomOf="@id/tv_text_sale_count"/>

    <!--退货单-->
    <TextView
        android:id="@+id/tv_text_refund_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_refund_count"
        style="@style/DetailTextStyle"
        app:layout_constraintStart_toStartOf="@id/tv_text_sale_count"
        app:layout_constraintTop_toTopOf="@id/tv_text_alipay"
        app:layout_constraintBottom_toBottomOf="@id/tv_text_alipay"/>

    <TextView
        android:id="@+id/tv_refund_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        style="@style/DetailTextStyle"
        app:layout_constraintEnd_toEndOf="@id/tv_sale_count"
        app:layout_constraintTop_toTopOf="@id/tv_text_refund_count"
        app:layout_constraintBottom_toBottomOf="@id/tv_text_refund_count"/>





    <!--总现金-->
    <TextView
        android:id="@+id/tv_text_total_cash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_total_cash"
        style="@style/HandoverTextStyle"
        app:layout_constraintStart_toEndOf="@id/right"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/tv_text_total_sales"
        app:layout_constraintBottom_toBottomOf="@id/tv_text_total_sales"/>

    <TextView
        android:id="@+id/tv_total_cash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.00"
        style="@style/HandoverMoneyTextStyle"
        app:layout_constraintStart_toStartOf="@id/tv_text_total_cash"
        app:layout_constraintEnd_toEndOf="@id/tv_text_total_cash"
        app:layout_constraintTop_toTopOf="@id/tv_total_sales"
        app:layout_constraintBottom_toBottomOf="@id/tv_total_sales"/>

