        android:text="0.00"
        style="@style/DetailTextStyle"
        android:layout_marginEnd="@dimen/x13"
        app:layout_constraintEnd_toEndOf="@id/right_line_1"
        app:layout_constraintTop_toTopOf="@id/tv_text_receipts"
