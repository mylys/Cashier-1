    <!--找零金额-->
    <TextView
        android:id="@+id/tv_text_change"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_cash_change"
        style="@style/DetailTextStyle"
        android:layout_marginStart="@dimen/x13"
        app:layout_constraintStart_toStartOf="@id/right_line_1"
        app:layout_constraintTop_toTopOf="@id/tv_text_wechat"
        app:layout_constraintBottom_toBottomOf="@id/tv_text_wechat"/>

    <TextView
        android:id="@+id/tv_change"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.00"
        style="@style/DetailTextStyle"
        android:layout_marginEnd="@dimen/x13"
        app:layout_constraintEnd_toEndOf="@id/right_line_1"
        app:layout_constraintTop_toTopOf="@id/tv_text_wechat"
        app:layout_constraintBottom_toBottomOf="@id/tv_text_wechat"/>

    <!--现金收入-->
    <TextView
        android:id="@+id/tv_text_cash_income"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_cash_income"
        style="@style/DetailTextStyle"
        android:layout_marginStart="@dimen/x13"
        android:layout_marginBottom="@dimen/y22"
        app:layout_constraintStart_toStartOf="@id/right_line_1"
        app:layout_constraintBottom_toTopOf="@id/right_line_4"/>

    <TextView
        android:id="@+id/tv_cash_income"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.00"
        style="@style/DetailTextStyle"
        android:layout_marginEnd="@dimen/x13"
        app:layout_constraintEnd_toEndOf="@id/right_line_1"
        app:layout_constraintTop_toTopOf="@id/tv_text_cash_income"
        app:layout_constraintBottom_toBottomOf="@id/tv_text_cash_income"/>

    <!--退款现金-->
    <TextView
        android:id="@+id/tv_text_cash_refund"
        android:layout_width="wrap_content"
